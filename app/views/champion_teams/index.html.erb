<p id="notice"><%= notice %></p>

<h1>Champion Teams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @champion_teams.each do |champion_team| %>
      <tr>
        <td><%= link_to 'Show', champion_team %></td>
        <td><%= link_to 'Edit', edit_champion_team_path(champion_team) %></td>
        <td><%= link_to 'Destroy', champion_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Champion Team 1', new_champion_team_path %>
