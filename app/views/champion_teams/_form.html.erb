
<%= form_tag users_path do %>
  <% @users_arr.each do |user| %>
    <%= fields_for 'users_container[]', user do |p| %>

      <div class="field">
        <%= p.label :name %><br>
        <%= p.text_field :name %>
      </div>

    <% end %>
  <% end %>
  <div class="actions">
    <%= submit_tag %>
  </div>
<% end %>
